{
  "name": "@astrojs/html",
  "description": "Add HTML pages to your Astro site",
  "version": "0.0.1",
  "type": "module",
  "types": "./dist/index.d.ts",
  "author": "withastro",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/withastro/astro.git",
    "directory": "packages/integrations/node"
  },
